[user]
  name = Matthew Boston
  email = matthew.boston@gmail.com
[github]
  user = bostonaholic
[color]
  ui = auto
[push]
  default = matching
[alias]
  aa = add --all
  ammend = commit --amend
  b = branch
  ci = commit
  co = checkout
  dc = diff --cached
  di = diff
  fa = fetch --all
  h = head
  head = !git l -1
  l = "!. ~/.githelpers && pretty_git_log"
  la = !git l --all
  pom = push origin master
  r = !git l -30
  ra = !git r --all
  st = status
  u = !git upstream
  upstream = "!git fetch upstream; git merge upstream/master"
