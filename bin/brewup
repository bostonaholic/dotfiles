#!/usr/bin/env bash

function brew_command () {
    echo -e '\n==>' brew $* '\n'
    brew $*
}

brew_command update && \
  brew_command upgrade && \
  brew_command cask upgrade && \
  brew_command doctor && \
  brew_command cask doctor && \
  brew_command outdated && \
  brew_command cask outdated && \
  brew_command cleanup
