#!/bin/bash
################################################################################
#
# rbenv Plugins
#
################################################################################

mkdir -p "$(rbenv root)/plugins"

git clone git@github.com:rbenv/rbenv-default-gems.git "$(rbenv root)/plugins/rbenv-default-gems"
git clone git@github.com:tpope/rbenv-ctags "$(rbenv root)/plugins/rbenv-ctags"
